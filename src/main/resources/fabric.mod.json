{
  "schemaVersion": 1,
  "id": "craftable_enchants",
  "version": "${version}",
  "name": "Craftable Enchantments",
  "description": "Adds a fun way to craft enchantments to the game through the use of an Enchantment Crafting Table and Enchantment Fragments",
  "authors": [
    "Daniel K."
  ],
  "contact": {
    "issues": "https://github.com/GitWither/craftable-enchants/issues",
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/fabric-craftable-enchantments",
    "discord": "https://discord.gg/WtdP245E8Q",
    "sources": "https://github.com/GitWither/craftable-enchants"
  },
  "license": "MIT",
  "icon": "assets/craftable_enchants/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "daniel.craftable_enchants.CraftableEnchants"
    ],
    "client": [
      "daniel.craftable_enchants.CraftableEnchants"
    ],
    "fabric-gametest" : [
      "daniel.craftable_enchants.test.CraftableEnchantsTestSuite"
    ]
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/b79VpMbkey",
        "modmenu.twitter": "https://twitter.com/_Daniel_K_"
      }
    }
  },
  "mixins": [
    "craftable_enchants.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.2",
    "fabric": "*",
    "minecraft": "1.19.x"
  }
}
